{
  "name": "snippetbox-numpy",
  "displayName": "snippetbox-numpy",
  "publisher": "VishvamRamani",
  "description": "Ultra-fast, alias-aware NumPy snippets for Python with automatic import detection and zero editor lag.",
  "version": "0.1.0",
  "icon": "logo.png",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:python"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [],
    "languages": [
      {
        "id": "python",
        "aliases": [
          "Python"
        ],
        "extensions": [
          ".py"
        ]
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.107.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "eslint": "^9.39.1",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
